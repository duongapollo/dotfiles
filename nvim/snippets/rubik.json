{
  "Rubik short comment": {
		"prefix": "rb*",
		"body": ["/* $1 */"]
	},
	"Rubik dash comment": {
		"prefix": "rb--",
		"body": ["/* ----- $1 ----- */"]
	},
	"Rubik slash comment": {
		"prefix": "rb/",
		"body": "// --"
	},
	"Console Debug": {
		"prefix": "debug",
		"body": "console.debug($1)"
	}
}
